{
  "redirects": [
    {
      "source": "http://letranido.com/(.*)",
      "destination": "https://www.letranido.com/$1",
      "permanent": true
    },
    {
      "source": "http://www.letranido.com/(.*)",
      "destination": "https://www.letranido.com/$1", 
      "permanent": true
    },
    {
      "source": "https://letranido.com/(.*)",
      "destination": "https://www.letranido.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
