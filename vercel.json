{
  "redirects": [
    {
      "source": "http://letranido.com/(.*)",
      "destination": "https://letranido.com/$1",
      "permanent": true
    },
    {
      "source": "http://www.letranido.com/(.*)", 
      "destination": "https://letranido.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ]
}
